{
  "service_id": "deg-browser",
  "service_description": "Differential expression lookup site",
  "version": "1.0.0",

  "doi": "",
  "docs_url": "https://deg-browser.org/docs",

  "mcp_url": "https://deg-browser.org/mcp",       // optional
  "api_url": "https://deg-browser.org/api/search",// optional
  "http_method": "GET",
  "timeout_sec": 15,

  "html_route": "/search?q={gene}",               // fallback UI route
  "query_param": "gene",
  "query_example": "TP53",

  "response_selector": "#deg-table",
  "field_map": {
    "gene":   "td:nth-child(1)",
    "log2FC": "td:nth-child(2)"
  }
}